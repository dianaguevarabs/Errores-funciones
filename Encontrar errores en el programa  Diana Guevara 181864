// Encontrar errores en el programa  Diana Guevara 181864

#include <stdio.h> // le flata el simbolo (#)
int suma (int, int);
int main ()// tiene doble t (intt) 
{
	int num1, num2, sum; // flata declarar la variable num2
	printf ("Imtroducir dos numeros a sumar \n"); // le flata el simbolo (;)
	scanf ("%d %d", &num1, &num2); // le faltan los simbolos (&)
	sum = suma (num1, num2);
	printf ("La suma es :%d", sum);
	return 0;
}
int suma (int a, int b)// tenia void al inicio,  (int **a), (,) y (;)
{
	int add;
    add = a + b;
    return add;
}
